{
  "name": "wokibrain",
  "version": "1.0.0",
  "description": "Enterprise-grade restaurant booking engine with intelligent table allocation",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "docs:websockets": "npx -y @asyncapi/cli@4.1.1 generate fromTemplate asyncapi.yaml @asyncapi/html-template@1.0.0 -o public/websockets-docs.html --force-write",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t wokibrain:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "migration:run": "tsx src/infrastructure/database/migrations/run.ts",
    "docs:build": "redocly build-docs openapi.yaml --output docs/index.html",
    "docs:serve": "redocly preview-docs openapi.yaml",
    "docs:validate": "redocly lint openapi.yaml",
    "grpc:generate": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=src/infrastructure/grpc src/infrastructure/grpc/booking.proto",
    "kafka:topics": "tsx src/infrastructure/messaging/create-topics.ts",
    "projections:rebuild": "tsx src/infrastructure/projections/rebuild.ts",
    "benchmark": "tsx benchmarks/load-test.ts",
    "benchmark:k6": "k6 run benchmarks/stress-test.k6.js",
    "benchmark:artillery": "artillery run benchmarks/artillery-config.yml",
    "benchmark:concurrency": "tsx benchmarks/concurrency-test.ts",
    "benchmark:regression": "tsx benchmarks/performance-regression.ts",
    "benchmark:all": "npm run benchmark && npm run benchmark:concurrency && npm run benchmark:regression",
    "postman:run": "newman run postman/WokiBrain-API.postman_collection.json -e postman/WokiBrain-Local.postman_environment.json",
    "postman:report": "newman run postman/WokiBrain-API.postman_collection.json -e postman/WokiBrain-Local.postman_environment.json -r htmlextra --reporter-htmlextra-export postman/newman-reports/report.html",
    "profile": "clinic doctor -- node dist/index.js",
    "profile:flame": "clinic flame -- node dist/index.js",
    "monitoring:dashboard": "open http://localhost:9464/metrics",
    "commit": "cz",
    "postinstall": "node -e \"try { const isCI = require('is-ci'); if (!isCI && require.resolve('husky')) require('husky').install(); } catch(e) {}\"",
    "prepare": "node -e \"try { const isCI = require('is-ci'); if (!isCI && require.resolve('husky')) require('husky').install(); } catch(e) {}\""
  },
  "keywords": [
    "restaurant",
    "booking",
    "reservation",
    "ddd",
    "fastify",
    "mongodb"
  ],
  "author": "Alan Cardozo <cardozoalani@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.654.0",
    "@fastify/auth": "^4.6.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "@grpc/grpc-js": "^1.14.2",
    "@grpc/proto-loader": "^0.8.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-prometheus": "^0.52.1",
    "@opentelemetry/instrumentation": "^0.52.1",
    "@opentelemetry/instrumentation-fastify": "^0.38.0",
    "@opentelemetry/instrumentation-mongodb": "^0.46.0",
    "@opentelemetry/sdk-metrics": "^1.25.1",
    "@opentelemetry/sdk-node": "^0.52.1",
    "bullmq": "^5.65.1",
    "eventemitter2": "^6.4.9",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "i18next": "^23.15.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongodb": "^6.9.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prom-client": "^15.1.0",
    "socket.io": "^4.8.1",
    "yaml": "^2.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@asyncapi/cli": "^2.14.0",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@faker-js/faker": "^10.1.0",
    "@redocly/openapi-core": "^2.12.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.2",
    "artillery": "^2.0.27",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "k6": "^0.0.0",
    "lint-staged": "^15.5.2",
    "mongodb-memory-server": "^10.4.1",
    "prettier": "^3.3.3",
    "redoc-cli": "^0.13.21",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yaml,yml,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
